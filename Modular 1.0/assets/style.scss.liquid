.nav-container {
  @include blockFix;
  bottom: 0;
  height: 100%;
  right: 100%;
  top: 0;
  width: 100%;
  z-index: 1000;
  @media screen and (min-width: $sm){
    width: 100%;
  }
  @media screen and (min-width: $med){
    width: 33%;
  }
  @media screen and (min-width: $xl){
    width: 20%;
  }
}
.nav-inner {
  @include blockAb;
  background: inherit;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
  width: 100%;
  -webkit-overflow-scrolling: touch;
  -webkit-transition: -webkit-transform .3s;
  -moz-transition: -moz-transform .3s;
  -ms-transition: -ms-transform .3s;
  transition: transform .3s;
  background-color: {{settings.drawer_bg_color}};
  h4, i, a {color: {{settings.drawer_text_color}};}
}
.nav-search {
  display: block;
  min-height: 16%;
  padding: 5px 0 55px;
  margin-top: 50px;
  position: static;
  text-align: center;
  z-index: 2;
  margin-left: -25px;

  .searchToggle {
    @include blockRel;
    cursor: pointer;
    z-index: 2;
      width: 66%;
      margin: auto;
  }

  .icon-close, .icon-search {
    font-size: 1rem;
    width: 1.3rem;
    vertical-align: middle;
  }
  .icon-close {
    display: none;
  }
  .h4 {
    display: inline-block;
    vertical-align: middle;
    margin-left: 12px;
    font-family: $header_font;
  }

  /**
   * Applied to .nav-container.
   */
  .search--is-visible & {
    .searchToggle {
      .icon-search {
        display: none;
      }
      .icon-close {
        display: inline-block;
      }
    }
    .nav-search-overlay {
      bottom: 0;
      opacity: 1;
    }
  }

  .search--is-hiding & {
    .searchToggle {
      .icon-search {
        display: inline-block;
      }
      .icon-close {
        display: none;
      }
    }
    .nav-search-overlay {
      opacity: 0;
    }
  }

  /**
   * Overlay contains search input
   */
  .nav-search-overlay {
    @include blockAb;
    @include transition(unquote('opacity .3s'));
    bottom: 100%; left: 0; right: 0; top: 0;
    opacity: 0;
    overflow: hidden;
    z-index: 1;
    .inputGroup {
      height: 40px;
      margin: 110px auto 0;
      width: 90%;
    }
    .nav-search-input {
      @include h4;
      height: 100%;
      line-height: 1;
      padding: 0 30px 0 0;
      width: 100%;
    }
    .nav-search-submit {
      @include blockAb;
      bottom: 0;
      right: -10px;
      top: 0;
      width: 40px;

      @media screen and (min-width: $med) {
        right: -25px;
        width: 60px;
      }
    }
  }
}

/**
 * The "X" that closes the nav on mobile.
 */
.nav-mobile-close {
  @include blockAb;
  font-size: 26px;
  height: 50px;
  line-height: 50px;
  right: 10px;
  top: 30px;
  text-align: center;
  width: 50px;
  z-index: 1;

  @media (min-width: ($med)){
    display: none;
  }
}

/*
 * Menu
 */
.menu {
  @include blockRel;
  min-height: 48%;
  text-align: center;
  width: 100%;
  padding-bottom: 35px;
  &.submenu--is-visible .menu-item > a, &.submenu--is-visible .menu-divider {
    opacity: 0;
    pointer-events: none;
    transform: translateX(-33%);
    -webkit-transform: translateX(-33%);
    -moz-transform: translateX(-33%);
    -ms-transform: translateX(-33%);
  }
}
.menu-item, .submenu-item {
  /**
   * Just the link is what get's animated out of frame.
   */
  & > a {
    @include transition(unquote('transform .3s ease-in-out, opacity .3s, color .2s'));
    display: inline-block;
    opacity: 1;
    width: 90%;
    line-height: 25px;
  }
}
.submenu-item > a {
  line-height: 20px;
}
.menu-item {
    @include h4;
    margin: .6em auto;
  &.has-submenu > a::after {
    @include blockAb;
    @include icon;
    content: "\e601";
    font-size: 0.6rem;
    right: -0.5em;
    top: 0;
  }
}
.menu-divider {
  @include inlineRel;
  @include transition(unquote('transform .3s ease-in-out, opacity .3s'));
  height: 1px;
  margin: 25px 0;
  width: 20px;
}

/**
 * Submenu
 */
.submenu {
  @include blockAb;
  @include transition(unquote('transform .3s ease-in-out, opacity .3s'));
  left: 0;
  opacity: 0;
  pointer-events: none;
  right: 0;
  top: 0;
  transform: translateX(33%);
  -webkit-transform: translateX(33%);
  -moz-transform: translateX(33%);
  -ms-transform: translateX(33%);
  .submenu--is-visible & {
    /** 
     * Only those .submenu's with this class animate in.
     */
    &.is-visible {
      opacity: 1;
      pointer-events: auto;
      transform: translateX(0);
      -webkit-transform: translateX(0);
      -moz-transform: translateX(0);
      -ms-transform: translateX(0);

    }
  }
}
.submenu-item {
  @include h4;
  margin-bottom: 10px;
}
.submenu-back {
  margin-bottom: 20px;
  opacity: 0.7;
  @include transition(opacity 150ms);
  &:hover { opacity: 1; }
  a {
    display: inline-block;
    width: 90%;
  }
  a:before {
    @include blockAb;
    @include icon;
    content: "\e600";
    font-size: 0.6rem;
    left: -0.5em;
    top: 0;
  }
}

/*
 * Footer
 */
.nav-footer {
  min-height: 38%;
  padding: 20px 0 10px;
}
.nav-social {
  margin: 10px 0;
  padding: 0 30px;
  text-align: center;

  .nav-social-link {
    @include inlineRel;
    font-size: 1.3rem;
    height: 40px;
    line-height: 40px;
    margin: 0.6em .3em;
    text-align: center;
    width: 40px;
    .diamond {
      opacity: 0.5;
    }
  }
}
.nav-signUp {
  padding: 30px;
    text-align: center;
}
.nav-signUp-form {
  @include blockRel;
  margin: 15px auto 0;
  width: 280px;
  @media screen and (min-width: $med){
    width: 360px;
  }
  &.success-response--is-visible, &.error-response--is-visible {
    .inputGroup {
      opacity: 0;
    }
  }
  .inputGroup {
    @include blockRel;
    height: 35px;
    width: 100%;
  }
}

/*
 * Login Items
 */
.login-links {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
  font-family: $header_font;
}

.ripple-click {
  -webkit-animation: anim-effect-jelena 0.3s ease-out forwards;
  animation: anim-effect-jelena 0.3s ease-out forwards;
}

@-webkit-keyframes anim-effect-jelena {
  0% {
    opacity: 1;
    -webkit-transform: scale3d(0.5, 0.5, 1) rotate(45deg);
    transform: scale3d(0.5, 0.5, 1) rotate(45deg);
  }
  to {
    opacity: 0;
    -webkit-transform: scale3d(1.2, 1.2, 1) rotate(45deg);
    transform: scale3d(1.2, 1.2, 1) rotate(45deg);
  }
}

@keyframes anim-effect-jelena {
  0% {
    opacity: 1;
    -webkit-transform: scale3d(0.5, 0.5, 1) rotate(45deg);
    transform: scale3d(0.5, 0.5, 1) rotate(45deg);
  }
  to {
    opacity: 0;
    -webkit-transform: scale3d(1.2, 1.2, 1) rotate(45deg);
    transform: scale3d(1.2, 1.2, 1) rotate(45deg);
  }
}

/*
 * Cart Popover
 */
.popover-container {
  @include blockFix;
  height: 0;
  top: 0;
  width: 100%;
  z-index: 1000;
}
.popover {
  @include blockRel;
  @include transition(unquote('.3s opacity ease-in-out'));
  background-color: $c-background;
  border: 1px solid $c-border;
  color: $c-text;
  margin: 0 auto;
  opacity: 0;
  top: 20px;

  &.is-visible {
    opacity: 1;
  }
}
.header-cartPopover {
  @include blockFix;
  @include transition(unquote('.3s opacity ease-in-out'));

  border: 1px solid $c-border;
  background-color: rgba($c-background, .97);
  color: $c-text;
  opacity: 0;
  right: 20px;
  top: 20px;
  z-index: 1000;

  @media (max-width: $lg){
    right: 0;
  }

  &.is-visible {
    opacity: 1;
  }
}
.popover-error {
  @include blockRel;
  @include h4;

  color: {{settings.color_secondary_new}};
  padding: 10px 20px;
  text-align: left;
  width: 350px;
}
.popover-inner {
  @include blockRel;

  cursor: pointer; // clickable to open cart
  padding: 10px 20px 20px;
  text-align: left;
  width: 350px;

  &::before,
    &::after {
      content: " ";
      display: table;
    }

    &::after {
      clear: both;
    }
}
.popover-item-thumb {
  @include blockRel;

  background-color: $c-border;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  height: 75px;
  float: left;
  margin-right: 20px;
  width: 75px;
}
.popover-item-info {
  @include blockRel;

  float: left;
}
.viewCart {
  @include blockRel;

  margin-bottom: 0;
  padding: 0 20px 10px;
  text-align: right;
  width: 100%;
}


// Variant
#VariantPopoverContainer {
  // when product images are in slideshow, hide popover
  display: none;
  text-align: center;
  @media (min-width: $med){
    display: block;
  }
  &:hover {
    .icon-arrow-left {
      color: rgba($c-text, 1);
    }
  }
  .popover {
    @include transition(unquote('opacity 0.5s cubic-bezier(.71,.01,.27,1), transform 0.5s cubic-bezier(.71,.01,.27,1)'));
    display: inline-block;
    opacity: 1;
    margin-top: 20px;
    min-width: 160px;
    opacity: 0;
    pointer-events: none;
    transform: translateY(calc(75px));
      -webkit-transform: translateY(calc(75px));
      -moz-transform: translateY(calc(75px));
      -ms-transform: translateY(calc(75px));

    &.is-visible {
      opacity: 1;
