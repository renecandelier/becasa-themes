// first param is the hover color, second is the text color (probably the global text color $c-text)
@function hover-color($hover, $text){
  @if lightness($hover) > 50 and lightness($text) > 50 {
    @return #222222;
  } @else if lightness($hover) < 50 and lightness($text) > 50 {
    @return $text;
  } @else if lightness($hover) > 50 and lightness($text) < 50 {
    @return $text;
  } @else {
    @return #FFFFFF;
  }
}

@function overlay-color($background, $desired_color){
  @if lightness($background) > 50 and lightness($desired_color) > 50 {
    @return #222222;
  } @else if lightness($background) < 50 and lightness($desired_color) > 50 {
    @return $desired_color;
  } @else if lightness($background) > 50 and lightness($desired_color) < 50 {
    @return $desired_color;
  } @else {
    @return #FFFFFF;
  }
}

// used to determine slider arrow backgrounds in
// instagram, gallery modules
@mixin check_opacity($color, $fallback) {
  @if alpha($color) > 0 {
    background-color: $color;
  } @else {
    background-color: $fallback;
  }
}


