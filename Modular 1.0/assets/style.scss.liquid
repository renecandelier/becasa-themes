    @include inlineRel;
    content: 'flickity';
    /**
     * Hiding the 'content' attr.
     */
    font-size: 0;
    width: 100%;
    @media screen and (min-width: $med){
      @include inlineRel;
      content: '';
      width: 100%;
    }
  }
}
/**
 * Product Images and Layouts
 */
.product-image {
  @include transition(unquote('opacity 0.3s ease-in-out'));
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  margin-bottom: 5px;
  padding-top: 0%;
  &.zoom--is-loading {
    &:after {
      @include blockAb;
      //****background-image: url('{{'load-more.gif' | asset_url }}');
      background-position: center;
      background-repeat: no-repeat;
      background-size: 79px 64px !important;
      bottom: 0;
      content: '';
      left: 0;
      right: 0;
      top: 0;
    }
    .product-image-zoom {
      background-color: $dark-overlay;
      .image-hover-light & {
        background-color: $light-overlay;
      }
    }
  }
  &.zoom--is-visible {
    img {
      @include transition(unquote('opacity 0.3s ease'));
      opacity: 1 !important;
    }
    .product-image-zoom {
      background-color: $dark-overlay;
      .image-hover-light & {
        background-color: $light-overlay;
      }
    }
  }
  &.zoom--is-hiding {
    img {
      opacity: 0 !important;
    }
    img.product-image-img {
      opacity: 1 !important;
    }
    .product-image-zoom {
      background-color: transparent !important;
    }
  }
  /**
   * Trigger for product image zoom
   */
  .product-image-zoom {
    @include transition(unquote('background-color 0.3s ease'));
    @include blockAb;
    background-color: transparent;
    bottom: 0;
    cursor: -moz-zoom-in;
    cursor: -webkit-zoom-in;
    cursor: url('./magnify.cur'), move;
    left: 0;
    opacity: .8;
    right: 0;
    top: 0;
    @media (max-width: ($med - 1)){
      background-color: rgba(#FFFFFF, .8);
      cursor: pointer;
      height: 40px;
      right: auto;
      text-align: center;
      top: auto;
      width: 40px;
      /**
       * Disable on mobile... for now.
       */
      display: none !important;
    }
  }
  /**
   * Show selected image flag.
   */
  &.is-selected-product {
    .product-selected-mark, .product-selected-flag {
      opacity: 1;
      pointer-events: auto;
    }
  }
  /**
   * To fade out and back in after clicking on Variant Popover.
   */
  &.fadeOut {
    opacity: 0.3;
    &.is-selected-product {
      opacity: 1;
    }
  }
  /**
   * IE8 doesn't support opacity.
   */
  .ie8 & {
    .product-selected-flag {
      opacity: 1;
      display: none;
    }
    &.is-selected-product {
      .product-selected-flag {
        display: block;
      }
    }
  }
  /**
   * Layout Classes
   * 25% width: calc(25% - #{$gutter} * 1.333333);
   */
  .product-layout-1 & {
    @media screen and (min-width: $med){
      &:nth-of-type(3n),
      &:nth-of-type(3n-1) {
        float: left;
        padding-top: 33%;
