/**
 * Selected image flag.
 */
.product-selected-flag {
  @include transition(unquote('opacity 0.3s ease'));
  background-color: $c-secondary;
  color: #FFFFFF;
  opacity: 0;
  pointer-events: none;
  &.is-visible {
    opacity: 1;
    pointer-events: auto;
  }
}
.product-selected-mark {
  @include transition(unquote('opacity 0.3s ease'));
  @include blockAb;
  background-color: $c-secondary;
  border-radius: 100px;
  opacity: 0;
  pointer-events: none;
  top: 15px; left: 15px;
  width: 30px; height: 30px;
  z-index: 1;
  &:after {
    @include blockAb;
    border-bottom: 3px solid #FFFFFF;
    border-left: 3px solid #FFFFFF;
    content: '';
    height: 6px;
    top: 9px; left: 8px;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
    width: 12px;
  }
  &.is-visible {
    opacity: 1;
    pointer-events: auto;
  }
}
.product-image-inner {
  @include blockAb;
  bottom: 0; left: 0; right: 0; top: 0;
  overflow: hidden;
  .product-image--fit & {
    right: -3px;
    text-align: center;
    &:after {
      @include inlineRel;
      content: '';
      height: 100%;
      vertical-align: middle;
    }
    .product-image-img {
      @include inlineRel;
      max-height: 90%;
      max-width: 90%;
      opacity: 1;
      vertical-align: middle;
    }
  }
  .product-image-img {
    @include transition(unquote('opacity .3s'));
    @include inlineRel;
    height: auto;
    opacity: 0;
  }
  .product-image-imagefill {
    @include blockAb;
    bottom: 0;
    left: 0;
    right: 0;
    top: 0;
    overflow: hidden;
  }
}
.product-info {
  padding: 40px 0 0;
  @media screen and (min-width: $lg){
    padding: 0;
  }
  .product-title {
    color: $c-heading;
  }
  .product-variant-preview {
    @include transition(unquote('height .3s'));
    @include blockRel;
    background-position: center;
    background-repeat: no-repeat;
    background-size: contain;
    cursor: pointer;
    height: 0;
    margin-bottom: 20px;
    opacity: 0;
    overflow: hidden;
    pointer-events: none;
    width: 280px;
    @media (max-width: $med - 1){
      display: none;
    }
    &.is-visible {
      height: 200px;
      opacity: 1;
      pointer-events: auto;
    }
    img {
      width: 100%;
      height: auto;
      opacity: 0;
    }
  }
}
.product-form {
  padding: 0 0 85px 0;
  @media screen and (min-width: $lg){
    padding: 0 0 85px 0;
  }
  .product-share {
    @include blockRel;
    color: $c-text;
    padding: 30px 0 20px;
    padding-top: 25px;
    a {
      @include transition(.2s opacity);
      color: $c-text;
      &:hover {
        color: 0.8;
      }
    }
  }
  .product-description {
    @include blockRel;
  }
  /**
   * Tabs
   */
  .product-tabs {
    margin: 0 0 20px;
  }
  .product-tab {
    @include inlineRel;
    @include h4;
    @include transition(.2s);
    border-bottom: 2px solid transparent;
    color: rgba($c-text, .6);
    cursor: pointer;
    margin-right: 15px;
    margin-bottom: 15px;
    padding: 1px 0;
    text-transform: uppercase;
    /**
     * Active class needs to be applied to first tab in markup.
     */
    &.is-active {
      border-bottom: 2px solid rgba($c-text, 1);
      color: rgba($c-text, 1);
    }
  }
  .product-tabber {
    -webkit-user-select: initial;
    -khtml-user-select: initial;
    -moz-user-select: initial;
    -ms-user-select: initial;
    user-select: initial;
    .slick-slide {
      background-color: $c-background;
      height: auto;
    }
  }
}

/**
 * Zoom.
 */
.mobile-zoom {
  @include blockAb;
  background-color: $dark-overlay;
  bottom: 0;
  color: hover-color($dark-overlay, $c-text);
  cursor: pointer;
  height: 50px;
  left: 0;
  text-align: center;
  width: 50px;
  @media (min-width: $med){
    display: none;
  }
  .icon-zoom {
    font-size: 20px;
    line-height: 50px;
  }
  .image-hover-light & {
    background-color: $light-overlay;
    color: hover-color($light-overlay, $c-text);
  }
}
.mobile-zoom-overlay {
  @include blockFix;
  @include transition(unquote('opacity .3s'));
  background-color: rgba($c-background, .9);
  bottom: 100%; 
  left: 0;
  opacity: 0;
  right: 0;
  top: 0;
  z-index: 1000;
  @media (min-width: $med){
    display: none;
  }
  &.is-visible {
    bottom: 0;
    opacity: 1;
  }
  &.is-hiding {
    opacity: 0;
  }
  img {
    @include blockRel;
    @include transition(unquote('opacity .3s'));
    margin: 0 auto;
    max-width: 95%;
    opacity: 0;
    z-index: 0;
    &.fade-in {
      opacity: 1;
    }
  }
  .mobile-zoom-close {
    @include blockAb;
    background-color: $c-text;
    color: $c-background;
    cursor: pointer;
    display: none;
    height: 50px;
    right: 0;
    text-align: center;
    top: 0;
    width: 50px;
    z-index: 1;
    .icon-close {
      font-size: 20px;
      line-height: 50px;
    }
    .image-hover-light & {
      background-color: $light-overlay;
      color: hover-color($light-overlay, $c-text);
    }
  }
 
}

.is-visible .mobile-zoom-close {
