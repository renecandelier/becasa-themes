/**
 * Menu Icon
 */
.menuToggle {
  @include transition( unquote('opacity .1s, background .2s ease, left .3s') );
  display: block;
  //.menuToggle
  float: left;
  font-size: 20px;
  margin-top: 12px;
  padding: 22px 10px 12px;
  position: relative;
  z-index: 100;
  .icon-bar,
  .icon-bar:before,
  .icon-bar:after {
  //  background-color: $c-text;
  }
  /**
   * Square behind hamburger, rotates when menu is open.
   */
  .diamond-bar {
    background-color: $c-background;
  }
  @media screen and (min-width: $lg) {
    margin-top: 0;
    left: 31px;
    top: 12px;
    /**
     * Hero only has border above $lg breakpoint even though this class will still be present.
     */
    .header--border & {
      left: 41px;
      top: 43px;
    }
  }
  /**
   * This class appears IF .hero is present, removes background color.
   */
  .header--no-bg & {
    .diamond-bar { background-color: rgba(white, 0); }
    .icon-bar,
    .icon-bar:before,
    .icon-bar:after {
    //  background-color: $c-text;
    }
  }
}

/**
 * Cart Icon
 */
.cartToggle {
  @include transition( unquote('background .2s ease, right .3s') );
  color: $c-text;
  display: block;
  float: right;
  height: 30px;
  margin-top: 38px;
  margin-right: 0;
  position: relative;
  right: 0px;
  // *** Changed width
  width: 30px;
  z-index: 1000;
  /**
   * Show "CART" text.
   */
  &:hover::after {
    opacity: 1;
    transform: translateY(0);
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -ms-transform: translateY(0);
  }
  /**
   * "CART" text.
   */
  &:after {
    @include blockAb;
    @include h5;
    @include transition( unquote('transform 0.1s, opacity .2s') );
    color: $c-text;
    content: ''; // *** Removed text from cart - {{ "general.header.cart_label" | t }}
    left: -10px;
    opacity: 0;
    right: -10px;
    top: 38px;
    text-align: center;
    transform: translateY(-15px);
    -webkit-transform: translateY(-15px);
    -moz-transform: translateY(-15px);
    -ms-transform: translateY(-15px);
    .header--no-bg & {
      color: $c-text;
    }
  }
  @media screen and (min-width: $lg) {
    margin-top: 0;
    right: 40px;
    top: 38px;
  }
  /**
   * Hero only has border above $xs breakpoint even though this class will still be present.
   */
  @media screen and (min-width: $lg) {
    .header--border & {
      right: 40px;
      top: 38px;
    }
  }
  /**
   * If .hero is present, remove background.
   */
  .header--no-bg & {
    background-color: rgba(white, 0);
    &.diamond-outline {
      &:hover .diamond {
        border-color: $c-text;
      }
      .diamond {
        background-color: transparent;
        border-color: $c-text;
      }
    }
  }

    /**
     * Child of .diamond-outline this is the bit that's visible and has the hover effect.
     */
  &.diamond-outline {
    &:hover .diamond {
      border-color: $c-text;
    }
    .diamond {
      background-color: $c-background;
      border-color: $c-text;
    }
  }

  /**
   * The number of items within the cartToggle.
   */
  .cartToggle-items {
    @include blockAb;
    @include h4;
    bottom: 0;
    color: {{settings.header_icon_color}};
    font-family: $header_font;
    left: 0;
    line-height: 1;
    padding: 8px 0;
    right: 0;
    text-align: center;
    letter-spacing: 0px !important;
  }
}

/*
 * Main Logo
 */
.main-logo {
  @include inlineRel;
  @include transition(unquote('opacity .3s, transform .3s, -webkit-transform .3s'));
  height: 70px;
  text-align: center;
