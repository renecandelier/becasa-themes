  &.style-nav-text {
    .slick-next,
    .slick-prev {
      background-color: #FFFFFF;
      transition: 0.3s opacity;
      &:after { color: $c-primary; }
    }
    .slick-dots {
      li { 
        border-color: $c-text;
        &.slick-active { background-color: $c-text; }
      }
    }
  }
  height:100vh;
  overflow: hidden;
  @media (max-width: $xxl) {
    height: auto;
    padding-top: 52%;
  }
  @media (max-width: $lg){
    padding-top: 70%;
  }
  @media (max-width: $med){
    padding-top: 120%;
  }
  @media (max-width: $xs){
    padding-top: 150%;
  }
  @media screen and (min-width: $med) {
    &:hover {
      .slick-prev, .slick-next {
        /**
         * Same as default state.
         */
        opacity:1; 
        &:hover { opacity: 1; }
      }
    }
  }
  &.hero--border {
    /**
     * Hero--border border removed under $xs.
     */
    @media screen and (min-width: $xs) {
      margin-top: 20px;
      margin-bottom: 20px;
      width: calc(100% - 40px);
    }
    .ie8 & {
      height: 800px;
      margin-top: 20px;
      width: 97%;
    }
  }
  /**
   * This remains if the slideshow is disabled too, so don't delete.
   */
  .hero-slide {
    @include blockAb;
    bottom: 0;
    left: 0;
    right: 0; 
    top: 0;
  }
  /*.slide-inner:not(.bg-loaded) .slide-content { opacity: 0; }*/
  .slide-content {
    padding: 30px;
    position: relative;
    h1,
    h2,
    .button { 
      position: relative;
      z-index: 9;
    }
    .button {
      @include transition(.3s opacity);
      margin-top: 15px;
      &:hover { opacity: 0.8; }
    }
    &:before {
      content: '';
      height: 100%;
      left: 0;
      opacity: 0.5;
      position: absolute;
      top: 0;
      width: 100%;
      z-index: 0;
    }
  }

  /**
   * Slider Related
   */
  .hero-dots-row {
    max-width: calc(1600px * 0.9);
    @media screen and (min-width: $xs) {
      max-width: calc(1600px * 0.875);
    }
    @media screen and (min-width: $med) {
      max-width: calc(1600px * 0.85);
    }
  }
  .slick-list {
    bottom: 0; left: 0; right: 0; top: 0;
    position: absolute;
  }
  .slick-prev, .slick-next {
    top: calc(50% - 40px);
  }
}

.hero-slider {
    visibility: visible;
    -webkit-transform: translateY(0) scale(1);
    opacity: 1;
    transform: translateY(0) scale(1);
    opacity: 1;
    -webkit-transition: -webkit-transform 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s, opacity 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s;
    transition: transform 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s, opacity 0.5s cubic-bezier(0.6, 0.2, 0.1, 1) 0s;
}

