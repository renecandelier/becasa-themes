/*
 * Custom slide styles
 */
.slide-inner {
    @include blockRel;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    height: 100%;
    width: 100%;
    .container {
        height: 100%;
        max-height: 100%;
        overflow: hidden;
        z-index: 999;
    }
    /* Vertically align the child, .heroObject-content
     * Also replaces default .row::after element
     */
    .row {
        height: 100%;
        &::after {
            @include inlineRel;
            content: '';
            height: 100%;
            vertical-align: middle;
        }
    }
