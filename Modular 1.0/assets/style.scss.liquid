// 3d animation keyframes

$spinner-3d-start: rotateY(0deg) translateZ($spinner-z-offset) rotateY(0deg);
$spinner-3d-mid: rotateY(-180deg) translateZ($spinner-z-offset) rotateY(180deg);
$spinner-3d-end: rotateY(-360deg) translateZ($spinner-z-offset) rotateY(360deg);

