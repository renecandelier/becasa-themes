    vertical-align: middle;
    @media (min-width: 700px){
      max-width: 700px;
    }
}
.slide-background {
    background-size: cover;
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 0;
    background-position: center center;
}
.slide-background--blank {
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  opacity: .3;
}
.slide-placeholder {
    background-color: #FAF9F9;
    position: absolute;
    svg {
        height: 80%;
        left: 50%;
        max-height: 100%;
        min-height: 400px;
        max-width: 100%;
